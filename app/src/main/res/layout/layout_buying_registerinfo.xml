<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:text="商标注册信息"
        android:textSize="18dp" />

    <TextView
        android:id="@+id/buying_registerinfo_applicationdate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="8dp"
        android:text="申请日期" />

    <TextView
        android:id="@+id/buying_registerinfo_registerdate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="8dp"
        android:text="注册日期" />

    <TextView
        android:id="@+id/buying_registerinfo_preliminarynoticedate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="8dp"
        android:text="初审公告日期" />

    <TextView
        android:id="@+id/buying_registerinfo_registrationnoticedate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="8dp"
        android:text="注册公告日期" />

    <TextView
        android:id="@+id/buying_registerinfo_dedicatedterm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="8dp"
        android:text="专用期限" />

</LinearLayout>
