<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/color_my_libs_f7f7f7"
    android:orientation="vertical">

    <RelativeLayout
        android:id="@+id/rl_webview_title"
        android:layout_width="match_parent"
        android:layout_height="@dimen/x88">

        <ImageView
            android:id="@+id/iv_webview_back"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/x88"
            android:layout_alignParentLeft="true"
            android:background="@android:color/transparent"
            android:paddingLeft="@dimen/x28"
            android:paddingRight="@dimen/x28"
            android:src="@mipmap/my_libs_ic_back_gray" />

        <TextView
            android:id="@+id/tv_webview_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:textColor="@android:color/black"
            android:textSize="@dimen/text_size_16" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/x1"
        android:background="@color/color_my_libs_e0e0e7" />


    <WebView
        android:id="@+id/wv_webview_content"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
</LinearLayout>
